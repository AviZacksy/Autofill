{
  "manifest_version": 3,
  "name": "Booking Auto-Fill",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://example-booking-site.com/*"],
      "js": ["content.js"]
    }
  ]
}
